#include<stdio.h>
main()
{
int i=0, j=0, k=0, i1=0, m, n, rs[30], flag=1, p[30];
system("clear");
printf("FIFOpagereplacementalgorithm....\\n");
printf("entertheno.offrames:");
scanf("%d"
,&n);
printf("enterthereferencestring:");
while(1)
{
scanf("%d"
,&rs[i]);
if(rs[i]==0)
break;
i++;
}
m=i;
for(j=0;j<n;j++)
p[j]=0;
for(i=0;i<m;i++)
{
flag=1;
for(j=0;j<n;j++)
if(p[j]==rs[i]){
printf("dataalreadyinpage....\n");
flag=0;
break;
}
if(flag==1){
p[i1]=rs[i];
i1++;
k++;
if(i1==n)
i1=0;
for(j=0;j<n;j++){
printf("\npage%d:%d",j+1,p[j]);
if(p[j]==rs[i])
printf("*");}
printf("\n\n");
}}
printf("totalnopagefaults=%d",k);}



